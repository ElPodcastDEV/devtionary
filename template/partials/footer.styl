footer
  margin-top: auto
  .browseZone
    border-bottom: 1px solid var(--secondary)
    padding: 20px 40px 
    background-color: var(--fill)
  ul, li
    list-style: none
    margin: 0
    padding: 0
  li
    display: inline
    a
      color: var(--secondary)
      font-size: 24px
      font-weight: 100
      &:hover
        text-decoration: underline
        color: var(--secondary)
  .footer
    text-align: center
    background-color: var(--darkFill)
    &, a
      color: var(--fill)
    padding: 40px
